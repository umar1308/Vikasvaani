{
	"version": 2,
	"builds": [
	  {
		"src": "ibmproject/wsgi.py",
		"use": "@vercel/python",
		"config": { "maxLambdaSize": "15mb" }
	  },
	  {
		"src": "manage.py",
		"use": "@vercel/python"
	  }
	],
	"routes": [
	  {
		"src": "/staticfiles/(.*)",
		"dest": "/static/$1"
	  },
	  {
		"src": "/(.*)",
		"dest": "ibmproject/wsgi.py"
	  }
	]
  }
